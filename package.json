{
  "name": "orangec-at",
  "version": "1.0.0",
  "repository": "https://github.com/orangec-at/orangec-at.git",
  "author": "orangec-at <radio941016@gmail.com>",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "blog": "pnpm --filter @orangec-at/blog",
    "design": "pnpm --filter @orangec-at/design",
    "build:vc": "vercel build --prod",
