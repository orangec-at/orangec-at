{
  "name": "orangec-at",
  "version": "1.0.0",
  "repository": "https://github.com/orangec-at/orangec-at.git",
  "author": "orangec-at <radio941016@gmail.com>",
  "workspaces": {
    "packages": [
      "apps/**",
      "packages/**"
    ]
  },
  "packageManager": "yarn@4.9.4",
  "scripts": {
    "blog": "yarn workspace @orangec-at/blog",
    "build:vc": "vercel build --prod",
    "deploy:prd": "vercel deploy --prod",
    "deploy:pre": "vercel deploy --prebuilt --prod",
    "vercel:pull": "vercel pull --yes",
    "vercel:dev": "vercel dev --yes",
    "clean:vercel": "rm -rf .vercel/output"
  }
}
